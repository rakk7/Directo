{
  "name": "kfirba/directo",
  "description": "A library to generate signatures for direct upload to Amazon S3",
  "keywords": [
    "AWS Signature V4",
    "S3 direct upload",
    "amazon S3 upload",
    "signature",
    "S3 signature",
    "S3 policy",
    "upload to S3"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Kfir Ben-Ami",
      "email": "kfirba2@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.0"
  },
  "require-dev": {
    "php": ">=7.1.0",
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "^0.9.5"
  },
  "autoload": {
    "psr-4": {
      "Kfirba\\Directo\\": "src/"
    }
  },
  "autoload-dev": {
    "files": [
      "tests/TestCase.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kfirba\\Directo\\Support\\DirectoServiceProvider"
      ],
      "aliases": {
        "Directo": "Kfirba\\Directo\\Support\\Facades\\Directo"
      }
    }
  }
}
